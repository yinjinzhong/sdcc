<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="sbittst" />
		<Option pch_mode="2" />
		<Option compiler="sdcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin\Debug\sbittst.hex" prefix_auto="1" extension_auto="0" />
				<Option object_output="obj\Debug\" />
				<Option type="1" />
				<Option compiler="sdcc" />
			</Target>
			<Target title="Release">
				<Option output="bin\Release\sbittst.hex" prefix_auto="1" extension_auto="0" />
				<Option object_output="obj\Release\" />
				<Option type="1" />
				<Option compiler="sdcc" />
			</Target>
		</Build>
		<Compiler>
			<Add option="--out-fmt-ihx" />
			<Add option="-mmcs51" />
		</Compiler>
		<ExtraCommands>
			<Add after="cmd /c type $(TARGET_OUTPUT_DIR)sbittst.mem" />
		</ExtraCommands>
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="main.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
